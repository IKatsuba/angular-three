{
  "npmScope": "angular-three",
  "affected": { "defaultBase": "main" },
  "implicitDependencies": {
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "core": { "tags": ["scope:core", "type:library"] },
    "core-materials": {
      "tags": ["scope:core", "type:library", "context:materials"]
    },
    "core-geometries": {
      "tags": ["scope:core", "type:library", "context:geometries"]
    },
    "core-meshes": { "tags": ["scope:core", "type:library", "context:meshes"] },
    "core-lights": { "tags": ["scope:core", "type:library", "context:lights"] },
    "core-loaders": {
      "tags": ["scope:core", "type:library", "context:loaders"]
    },
    "demo": { "tags": [] },
    "core-stats": { "tags": ["scope:core", "type:library", "context:stats"] },
    "controls": { "tags": ["scope:controls", "type:library"] },
    "core-attributes": {
      "tags": ["scope:core", "type:library", "context:attributes"]
    },
    "postprocessing": { "tags": ["scope:postprocessing", "type:library"] },
    "postprocessing-render-pass": {
      "tags": ["scope:postprocessing", "type:library", "context:render-pass"]
    },
    "core-canvas": { "tags": ["scope:core", "type:library", "context:canvas"] },
    "postprocessing-ssao-pass": {
      "tags": ["scope:postprocessing", "type:library", "context:ssao-pass"]
    },
    "postprocessing-unreal-bloom-pass": {
      "tags": [
        "scope:postprocessing",
        "type:library",
        "context:unreal-bloom-pass"
      ]
    },
    "postprocessing-shader-pass": {
      "tags": ["scope:postprocessing", "type:library", "context:shader-pass"]
    },
    "core-group": { "tags": ["scope:core", "type:library", "context:group"] },
    "core-scene": { "tags": ["scope:core", "type:library", "context:scene"] },
    "loaders": { "tags": ["scope:loaders", "type:library"] },
    "loaders-gltf-loader": {
      "tags": ["scope:loaders", "type:library", "context:gltf-loader"]
    },
    "core-audios": { "tags": ["scope:core", "type:library", "context:audios"] },
    "core-sprites": {
      "tags": ["scope:core", "type:library", "context:sprites"]
    }
  },
  "workspaceLayout": { "appsDir": "packages", "libsDir": "packages" }
}
