{
  "version": 2,
  "projects": {
    "controls": "packages/controls",
    "core": "packages/core",
    "demo": "packages/demo",
    "helpers": "packages/helpers",
    "popmotion": "packages/popmotion",
    "postprocessing": "packages/postprocessing"
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none",
      "strict": true
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "strict": true
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "core"
}
