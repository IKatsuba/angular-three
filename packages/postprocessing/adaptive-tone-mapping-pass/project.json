{
  "projectType": "library",
  "root": "packages/postprocessing/adaptive-tone-mapping-pass",
  "sourceRoot": "packages/postprocessing/adaptive-tone-mapping-pass/src",
  "prefix": "ngt",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "options": {
        "tsConfig": "packages/postprocessing/adaptive-tone-mapping-pass/tsconfig.lib.json",
        "project": "packages/postprocessing/adaptive-tone-mapping-pass/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/postprocessing/adaptive-tone-mapping-pass/tsconfig.lib.prod.json"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
